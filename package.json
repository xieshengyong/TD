{
  "name": "td",
  "version": "1.0.0",
  "description": "XXX-项目描述\r ========",
  "main": "",
  "publicUrl": {
    "dist": "/",
    "td": "https://qrss.treedom.cn/demo/",
    "tx": "//ossweb-img.qq.com/images/a20171121act/"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run dev",
    "dev": "parcel index.html -d .dev",
    "build": "npm run dist -- --no-content-hash",
    "build:td": "npm run dist -- %npm_package_publicUrl_td% --no-content-hash",
    "build:tx": "npm run dist -- %npm_package_publicUrl_tx% --no-content-hash",
    "predist": "npm run clean",
    "dist": "parcel build index.html --no-source-maps -d dist/ossweb-img --public-url",
    "postdist": "npm run movefile",
    "movefile": "cpy dist/ossweb-img/*.html dist && rimraf dist/ossweb-img/*.html",
    "removehash": "renamer --find \"/\\.\\w+\\./g\" --replace \".\" dist\\ossweb-img\\*.*",
    "pngquant": "pngquant --speed 1 --ext=.png --force src/img/**/*.png"
  },
  "dependencies": {
    "howler": "^2.1.2",
    "pixi.js": "^5.0.4",
    "zepto": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-preset-es2015": "^6.24.1",
    "cpy-cli": "^2.0.0",
    "less": "^3.9.0",
    "parcel-bundler": "^1.12.3",
    "pngquant": "^3.0.0",
    "postcss-pxtorem": "^4.0.1",
    "renamer": "^1.1.2",
    "rimraf": "^2.6.3"
  },
  "browserslist": [
    "last 1 version",
    "> 1% in CN",
    "IE 10"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xieshengyong/TD.git"
  },
